// 将设置放入此文件中以覆盖默认值和用户设置。
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "src/**/*.js.map": true,
        "src/**/*.js": true,
        "_*": true,
        "**/*.o": true,
        "**/*.d": true,
        "**/*.sym": true,
        "**/*.out": true,
        "**/*.asm": true,
        "**/exec_*": true
    },
    "git.ignoreLimitWarning": true
}